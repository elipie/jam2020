let Random = {
    func random(self, seed) {
        let rand;
        let v0:int = 12345+(seed*1103515245);
        rand = v0.to_int() & 2147483647;
        return rand;
    }
    
    func range(self, seed, high, low) {
        let rand = self.random(seed);
        return rand % (high-low+1)+low;
    }
};

