let Num = Type.extend({
  name = 'Num'

  func __construct__(self, value) {
      self._value = value;

      return self;
  }

  func to_int(self) {
    return Int.new(self._value);
  }

  func to_float(self) {
    return Float.new(self._value);
  }

  func __call__(self, args) {
    return self.__mul__(*args);
  }
});
